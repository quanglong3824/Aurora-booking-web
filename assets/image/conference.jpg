<svg width="400" height="300" viewBox="0 0 400 300" fill="none" xmlns="http://www.w3.org/2000/svg">
<defs>
<linearGradient id="conferenceGradient" x1="0%" y1="0%" x2="100%" y2="100%">
<stop offset="0%" style="stop-color:#1e40af;stop-opacity:1" />
<stop offset="100%" style="stop-color:#3b82f6;stop-opacity:1" />
</linearGradient>
</defs>
<rect width="400" height="300" fill="url(#conferenceGradient)"/>
<!-- Conference Room -->
<rect x="50" y="100" width="300" height="150" fill="#1e293b"/>
<rect x="60" y="110" width="280" height="130" fill="#334155"/>
<!-- Conference Table -->
<ellipse cx="200" cy="175" rx="100" ry="40" fill="#7c2d12"/>
<!-- Chairs around table -->
<circle cx="150" cy="150" r="12" fill="#dc2626"/>
<circle cx="200" cy="140" r="12" fill="#dc2626"/>
<circle cx="250" cy="150" r="12" fill="#dc2626"/>
<circle cx="150" cy="200" r="12" fill="#dc2626"/>
<circle cx="200" cy="210" r="12" fill="#dc2626"/>
<circle cx="250" cy="200" r="12" fill="#dc2626"/>
<!-- Projector Screen -->
<rect x="80" y="120" width="80" height="50" fill="#f3f4f6"/>
<rect x="85" y="125" width="70" height="40" fill="#e5e7eb"/>
<!-- Projector -->
<rect x="300" y="80" width="40" height="20" fill="#374151"/>
<circle cx="320" cy="90" r="5" fill="#fbbf24"/>
<!-- Ceiling Lights -->
<circle cx="150" cy="60" r="15" fill="#fbbf24"/>
<circle cx="250" cy="60" r="15" fill="#fbbf24"/>
<text x="200" y="280" text-anchor="middle" fill="#f8fafc" font-family="Arial" font-size="16" font-weight="bold">Conference Room</text>
</svg>