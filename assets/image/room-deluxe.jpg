<ellipse cx="270" cy="210" rx="40" ry="25" fill="#451a03"/>
<!-- Chandelier -->
<circle cx="200" cy="80" r="25" fill="#fbbf24"/>
<circle cx="200" cy="80" r="15" fill="#f59e0b"/>
<!-- Floor -->
<rect x="0" y="250" width="400" height="50" fill="#a3a3a3"/>
<text x="200" y="280" text-anchor="middle" fill="#374151" font-family="Arial" font-size="16" font-weight="bold">Phòng Cao Cấp</text>
</svg>