<line x1="220" y1="110" x2="340" y2="110" stroke="#3b82f6" stroke-width="3"/>
<!-- Luxury Chair -->
<rect x="190" y="170" width="50" height="70" fill="#7c2d12"/>
<rect x="195" y="175" width="40" height="40" fill="#a3a3a3"/>
