<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1200 600">
  <defs>
    <linearGradient id="galleryBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#764ba2;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="frameGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#FFD700;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#FFA500;stop-opacity:1" />
    </linearGradient>
    <radialGradient id="spotlight" cx="50%" cy="40%">
      <stop offset="0%" style="stop-color:#fff;stop-opacity:0.3" />
      <stop offset="100%" style="stop-color:#fff;stop-opacity:0" />
    </radialGradient>
  </defs>
  
  <!-- Background -->
  <rect width="1200" height="600" fill="url(#galleryBg)"/>
  
  <!-- Spotlight effect -->
  <ellipse cx="600" cy="240" rx="500" ry="300" fill="url(#spotlight)"/>
  
  <!-- Photo frames floating -->
  <g transform="rotate(-15 200 150)">
    <rect x="150" y="100" width="100" height="80" fill="url(#frameGradient)" rx="5"/>
    <rect x="160" y="110" width="80" height="60" fill="#fff"/>
    <rect x="165" y="115" width="70" height="50" fill="#E6E6FA"/>
    <text x="200" y="145" text-anchor="middle" font-family="Arial" font-size="8" fill="#666">ROOM</text>
  </g>
  
  <g transform="rotate(10 400 200)">
    <rect x="350" y="150" width="120" height="90" fill="url(#frameGradient)" rx="5"/>
    <rect x="360" y="160" width="100" height="70" fill="#fff"/>
    <rect x="365" y="165" width="90" height="60" fill="#F0F8FF"/>
    <text x="410" y="200" text-anchor="middle" font-family="Arial" font-size="10" fill="#666">SPA</text>
  </g>
  
  <g transform="rotate(-8 600 180)">
    <rect x="540" y="120" width="140" height="100" fill="url(#frameGradient)" rx="5"/>
    <rect x="550" y="130" width="120" height="80" fill="#fff"/>
    <rect x="555" y="135" width="110" height="70" fill="#F5F5DC"/>
    <text x="610" y="175" text-anchor="middle" font-family="Arial" font-size="12" fill="#666">RESTAURANT</text>
  </g>
  
  <g transform="rotate(12 800 220)">
    <rect x="740" y="170" width="110" height="85" fill="url(#frameGradient)" rx="5"/>
    <rect x="750" y="180" width="90" height="65" fill="#fff"/>
    <rect x="755" y="185" width="80" height="55" fill="#E0F6FF"/>
    <text x="795" y="215" text-anchor="middle" font-family="Arial" font-size="9" fill="#666">POOL</text>
  </g>
  
  <g transform="rotate(-20 1000 160)">
    <rect x="950" y="110" width="100" height="75" fill="url(#frameGradient)" rx="5"/>
    <rect x="960" y="120" width="80" height="55" fill="#fff"/>
    <rect x="965" y="125" width="70" height="45" fill="#FFF8DC"/>
    <text x="1000" y="150" text-anchor="middle" font-family="Arial" font-size="8" fill="#666">LOBBY</text>
  </g>
  
  <!-- Bottom row frames -->
  <g transform="rotate(18 250 400)">
    <rect x="200" y="350" width="90" height="70" fill="url(#frameGradient)" rx="5"/>
    <rect x="210" y="360" width="70" height="50" fill="#fff"/>
    <rect x="215" y="365" width="60" height="40" fill="#F0FFFF"/>
    <text x="245" y="388" text-anchor="middle" font-family="Arial" font-size="7" fill="#666">GYM</text>
  </g>
  
  <g transform="rotate(-12 450 420)">
    <rect x="390" y="370" width="130" height="95" fill="url(#frameGradient)" rx="5"/>
    <rect x="400" y="380" width="110" height="75" fill="#fff"/>
    <rect x="405" y="385" width="100" height="65" fill="#FFFACD"/>
    <text x="455" y="425" text-anchor="middle" font-family="Arial" font-size="11" fill="#666">CONFERENCE</text>
  </g>
  
  <g transform="rotate(25 700 450)">
    <rect x="640" y="400" width="105" height="80" fill="url(#frameGradient)" rx="5"/>
    <rect x="650" y="410" width="85" height="60" fill="#fff"/>
    <rect x="655" y="415" width="75" height="50" fill="#F5FFFA"/>
    <text x="692" y="445" text-anchor="middle" font-family="Arial" font-size="9" fill="#666">GARDEN</text>
  </g>
  
  <g transform="rotate(-15 950 430)">
    <rect x="900" y="380" width="95" height="75" fill="url(#frameGradient)" rx="5"/>
    <rect x="910" y="390" width="75" height="55" fill="#fff"/>
    <rect x="915" y="395" width="65" height="45" fill="#F0F0F0"/>
    <text x="947" y="420" text-anchor="middle" font-family="Arial" font-size="8" fill="#666">SUITE</text>
  </g>
  
  <!-- Camera icon -->
  <g transform="translate(580, 280)">
    <circle cx="20" cy="20" r="25" fill="#333" opacity="0.8"/>
    <rect x="10" y="15" width="20" height="15" rx="3" fill="#666"/>
    <rect x="5" y="10" width="30" height="20" rx="5" fill="#444"/>
    <circle cx="20" cy="20" r="8" fill="#222"/>
    <circle cx="20" cy="20" r="5" fill="#111"/>
    <rect x="25" y="12" width="4" height="3" rx="1" fill="#666"/>
  </g>
  
  <!-- Decorative elements -->
  <g fill="#FFD700" opacity="0.6">
    <circle cx="100" cy="100" r="3"/>
    <circle cx="1100" cy="120" r="2"/>
    <circle cx="150" cy="500" r="2.5"/>
    <circle cx="1050" cy="480" r="3"/>
    <circle cx="50" cy="300" r="2"/>
    <circle cx="1150" cy="350" r="2.5"/>
  </g>
  
  <!-- Light rays -->
  <g stroke="#FFD700" stroke-width="1" opacity="0.3" fill="none">
    <line x1="600" y1="50" x2="600" y2="100"/>
    <line x1="550" y1="60" x2="570" y2="90"/>
    <line x1="650" y1="60" x2="630" y2="90"/>
    <line x1="500" y1="80" x2="530" y2="110"/>
    <line x1="700" y1="80" x2="670" y2="110"/>
  </g>
</svg>