<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="presidentialBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#faf5ff"/>
      <stop offset="100%" style="stop-color:#f3e8ff"/>
    </linearGradient>
    <linearGradient id="goldAccent" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffd700"/>
      <stop offset="100%" style="stop-color:#f6ad55"/>
    </linearGradient>
    <linearGradient id="panoramicView" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#e6fffa"/>
      <stop offset="30%" style="stop-color:#81e6d9"/>
      <stop offset="70%" style="stop-color:#4fd1c7"/>
      <stop offset="100%" style="stop-color:#38b2ac"/>
    </linearGradient>
    <linearGradient id="royalBed" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffffff"/>
      <stop offset="100%" style="stop-color:#fefefe"/>
    </linearGradient>
  </defs>
  
  <!-- Presidential Suite Background -->
  <rect width="400" height="300" fill="url(#presidentialBg)"/>
  
  <!-- Marble Floor with Gold Inlays -->
  <rect x="0" y="220" width="400" height="80" fill="#f8f9fa"/>
  <g opacity="0.6">
    <path d="M0,230 Q150,220 300,230 T400,230" stroke="url(#goldAccent)" stroke-width="3" fill="none"/>
    <path d="M0,250 Q200,240 400,250" stroke="url(#goldAccent)" stroke-width="2" fill="none"/>
    <circle cx="120" cy="240" r="12" fill="url(#goldAccent)" opacity="0.4"/>
    <circle cx="280" cy="260" r="10" fill="url(#goldAccent)" opacity="0.4"/>
    <polygon points="200,235 210,245 200,255 190,245" fill="url(#goldAccent)" opacity="0.3"/>
  </g>
  
  <!-- Back Wall with Premium Finish -->
  <rect x="0" y="0" width="400" height="220" fill="#f7fafc"/>
  
  <!-- Floor-to-Ceiling Panoramic Windows -->
  <rect x="150" y="10" width="240" height="150" fill="url(#panoramicView)" stroke="#2d3748" stroke-width="5"/>
  
  <!-- Cityscape/Ocean View -->
  <g opacity="0.9">
    <!-- Sky -->
    <rect x="150" y="10" width="240" height="60" fill="#e6fffa"/>
    
    <!-- Distant cityscape -->
    <rect x="170" y="50" width="8" height="30" fill="#2d3748" opacity="0.4"/>
    <rect x="185" y="45" width="12" height="35" fill="#2d3748" opacity="0.4"/>
    <rect x="205" y="40" width="10" height="40" fill="#2d3748" opacity="0.4"/>
    <rect x="220" y="48" width="15" height="32" fill="#2d3748" opacity="0.4"/>
    <rect x="250" y="42" width="18" height="38" fill="#2d3748" opacity="0.4"/>
    <rect x="280" y="35" width="20" height="45" fill="#2d3748" opacity="0.4"/>
    <rect x="310" y="38" width="16" height="42" fill="#2d3748" opacity="0.4"/>
    <rect x="340" y="45" width="12" height="35" fill="#2d3748" opacity="0.4"/>
    <rect x="360" y="50" width="10" height="30" fill="#2d3748" opacity="0.4"/>
    
    <!-- Water/Ocean -->
    <rect x="150" y="80" width="240" height="80" fill="url(#panoramicView)"/>
    
    <!-- Waves and reflections -->
    <path d="M150,120 Q200,115 250,120 T390,120" stroke="#ffffff" stroke-width="2" fill="none" opacity="0.5"/>
    <path d="M150,140 Q220,135 290,140 T390,140" stroke="#ffffff" stroke-width="1" fill="none" opacity="0.3"/>
    
    <!-- Luxury yacht -->
    <ellipse cx="300" cy="130" rx="15" ry="4" fill="#ffffff" opacity="0.8"/>
    <rect x="295" y="125" width="10" height="8" fill="#2d3748" opacity="0.6"/>
    
    <!-- Birds -->
    <path d="M180,30 Q185,25 190,30" stroke="#2d3748" stroke-width="1" fill="none"/>
    <path d="M320,25 Q325,20 330,25" stroke="#2d3748" stroke-width="1" fill="none"/>
  </g>
  
  <!-- Window frame -->
  <rect x="150" y="10" width="240" height="150" fill="none" stroke="#2d3748" stroke-width="5"/>
  <line x1="270" y1="10" x2="270" y2="160" stroke="#2d3748" stroke-width="3"/>
  <line x1="150" y1="85" x2="390" y2="85" stroke="#2d3748" stroke-width="3"/>
  
  <!-- Royal King Bed -->
  <rect x="10" y="80" width="120" height="80" fill="url(#royalBed)" rx="15"/>
  <rect x="15" y="85" width="110" height="70" fill="#ffffff" rx="10"/>
  
  <!-- Gold-trimmed headboard -->
  <rect x="5" y="75" width="130" height="15" fill="url(#goldAccent)" rx="8"/>
  
  <!-- Luxury Pillows -->
  <rect x="25" y="90" width="25" height="15" fill="#4299e1" rx="4"/>
  <rect x="55" y="90" width="25" height="15" fill="#4299e1" rx="4"/>
  <rect x="85" y="90" width="25" height="15" fill="#4299e1" rx="4"/>
  
  <!-- Royal Decorative Pillows -->
  <rect x="35" y="110" width="20" height="12" fill="url(#goldAccent)" rx="3"/>
  <rect x="65" y="110" width="20" height="12" fill="#9f7aea" rx="3"/>
  <rect x="95" y="110" width="20" height="12" fill="#e53e3e" rx="3"/>
  
  <!-- Presidential Seating Area -->
  <rect x="10" y="180" width="80" height="40" fill="#4299e1" rx="10"/>
  <rect x="15" y="175" width="70" height="8" fill="#3182ce"/>
  
  <!-- Gold-trimmed armrests -->
  <rect x="5" y="180" width="10" height="40" fill="url(#goldAccent)" rx="5"/>
  <rect x="85" y="180" width="10" height="40" fill="url(#goldAccent)" rx="5"/>
  
  <!-- Executive Coffee Table -->
  <rect x="100" y="190" width="60" height="35" fill="#8b4513" rx="6"/>
  <rect x="105" y="185" width="50" height="10" fill="url(#goldAccent)"/>
  
  <!-- Luxury items on table -->
  <circle cx="120" cy="200" r="5" fill="url(#goldAccent)"/>
  <rect x="135" y="195" width="15" height="10" fill="#38a169"/>
  <circle cx="150" cy="210" r="4" fill="#e53e3e"/>
  
  <!-- Premium Bedside Tables -->
  <rect x="140" y="100" width="35" height="50" fill="#8b4513"/>
  <rect x="145" y="95" width="25" height="8" fill="url(#goldAccent)"/>
  <circle cx="157" cy="110" r="3" fill="url(#goldAccent)"/>
  <circle cx="157" cy="125" r="3" fill="url(#goldAccent)"/>
  
  <!-- Crystal Chandelier -->
  <circle cx="80" cy="10" r="25" fill="url(#goldAccent)" opacity="0.8"/>
  <circle cx="80" cy="10" r="20" fill="#ffffff" opacity="0.6"/>
  <g stroke="url(#goldAccent)" stroke-width="2" opacity="0.7">
    <line x1="80" y1="35" x2="80" y2="50"/>
    <line x1="60" y1="20" x2="45" y2="35"/>
    <line x1="100" y1="20" x2="115" y2="35"/>
    <line x1="65" y1="35" x2="50" y2="50"/>
    <line x1="95" y1="35" x2="110" y2="50"/>
    <circle cx="80" cy="55" r="3" fill="url(#goldAccent)"/>
    <circle cx="50" cy="40" r="2" fill="url(#goldAccent)"/>
    <circle cx="110" cy="40" r="2" fill="url(#goldAccent)"/>
  </g>
  
  <!-- Private Bar -->
  <rect x="300" y="180" width="80" height="60" fill="#2d3748"/>
  <rect x="305" y="175" width="70" height="10" fill="url(#goldAccent)"/>
  
  <!-- Bar stools -->
  <rect x="320" y="240" width="15" height="20" fill="#8b4513"/>
  <circle cx="327" cy="235" r="8" fill="#4299e1"/>
  <rect x="350" y="240" width="15" height="20" fill="#8b4513"/>
  <circle cx="357" cy="235" r="8" fill="#4299e1"/>
  
  <!-- Wine collection -->
  <rect x="310" y="185" width="5" height="15" fill="#e53e3e"/>
  <rect x="320" y="185" width="5" height="15" fill="#38a169"/>
  <rect x="330" y="185" width="5" height="15" fill="#ffd700"/>
  <rect x="340" y="185" width="5" height="15" fill="#9f7aea"/>
  <rect x="350" y="185" width="5" height="15" fill="#e53e3e"/>
  <rect x="360" y="185" width="5" height="15" fill="#4299e1"/>
  
  <!-- Premium TV Wall Mount -->
  <rect x="200" y="170" width="80" height="50" fill="#2d3748" rx="8"/>
  <rect x="205" y="175" width="70" height="40" fill="#1a202c"/>
  <rect x="200" y="165" width="80" height="8" fill="url(#goldAccent)"/>
  
  <!-- Luxury Curtains -->
  <rect x="140" y="0" width="20" height="170" fill="#9f7aea" opacity="0.4"/>
  <rect x="380" y="0" width="20" height="170" fill="#9f7aea" opacity="0.4"/>
  
  <!-- Persian Carpet -->
  <ellipse cx="200" cy="260" rx="120" ry="40" fill="#9f7aea" opacity="0.6"/>
  <ellipse cx="200" cy="260" rx="100" ry="30" fill="#805ad5" opacity="0.5"/>
  <ellipse cx="200" cy="260" rx="80" ry="20" fill="#6b46c1" opacity="0.4"/>
  <ellipse cx="200" cy="260" rx="60" ry="15" fill="url(#goldAccent)" opacity="0.3"/>
  
  <!-- Gold decorative elements -->
  <circle cx="50" cy="50" r="8" fill="url(#goldAccent)" opacity="0.6"/>
  <polygon points="350,50 360,60 350,70 340,60" fill="url(#goldAccent)" opacity="0.5"/>
  <rect x="30" y="30" width="15" height="15" fill="url(#goldAccent)" opacity="0.4" rx="3"/>
  
  <!-- Private Jacuzzi -->
  <ellipse cx="320" cy="120" rx="25" ry="15" fill="#4299e1" opacity="0.3"/>
  <ellipse cx="320" cy="120" rx="20" ry="12" fill="#63b3ed" opacity="0.4"/>
  <circle cx="315" cy="115" r="2" fill="#ffffff" opacity="0.6"/>
  <circle cx="325" cy="125" r="2" fill="#ffffff" opacity="0.6"/>
  
  <!-- Royal Crown Symbol -->
  <g transform="translate(370, 20)" fill="url(#goldAccent)" opacity="0.7">
    <polygon points="0,15 5,5 10,15 15,5 20,15 10,0"/>
    <circle cx="5" cy="8" r="2"/>
    <circle cx="10" cy="3" r="2"/>
    <circle cx="15" cy="8" r="2"/>
  </g>
</svg>