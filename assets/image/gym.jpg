<svg width="400" height="300" viewBox="0 0 400 300" fill="none" xmlns="http://www.w3.org/2000/svg">
<defs>
<linearGradient id="gymGradient" x1="0%" y1="0%" x2="100%" y2="100%">
<stop offset="0%" style="stop-color:#1f2937;stop-opacity:1" />
<stop offset="100%" style="stop-color:#374151;stop-opacity:1" />
</linearGradient>
</defs>
<rect width="400" height="300" fill="url(#gymGradient)"/>
<!-- Gym Floor -->
<rect x="0" y="200" width="400" height="100" fill="#111827"/>
<!-- Treadmills -->
<rect x="50" y="150" width="60" height="40" fill="#6b7280"/>
<rect x="130" y="150" width="60" height="40" fill="#6b7280"/>
<rect x="210" y="150" width="60" height="40" fill="#6b7280"/>
<!-- Weight Racks -->
<rect x="300" y="120" width="80" height="60" fill="#374151"/>
<circle cx="320" cy="140" r="8" fill="#dc2626"/>
<circle cx="340" cy="140" r="8" fill="#dc2626"/>
<circle cx="360" cy="140" r="8" fill="#dc2626"/>
<!-- Dumbbells -->
<rect x="80" y="220" width="30" height="8" fill="#6b7280"/>
<circle cx="75" cy="224" r="6" fill="#374151"/>
<circle cx="115" cy="224" r="6" fill="#374151"/>
<!-- Mirrors -->
<rect x="20" y="80" width="360" height="60" fill="#e5e7eb" opacity="0.3"/>
<!-- Ceiling Lights -->
<circle cx="100" cy="40" r="15" fill="#fbbf24"/>
<circle cx="200" cy="40" r="15" fill="#fbbf24"/>
<circle cx="300" cy="40" r="15" fill="#fbbf24"/>
<text x="200" y="280" text-anchor="middle" fill="#e5e7eb" font-family="Arial" font-size="16" font-weight="bold">Fitness Center</text>
</svg>